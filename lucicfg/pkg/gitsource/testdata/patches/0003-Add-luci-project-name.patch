From 7b1085476b595ec17c51ffb664766efa2d3b2a28 Mon Sep 17 00:00:00 2001
From: Exemplar Exemplaris <exemplar@example.com>
Date: Wed, 26 Mar 2025 09:45:48 -0700
Subject: [PATCH] Add luci project name

---
 subdir/generated/project.cfg | 16 ++++++++++++++++
 subdir/generated/realms.cfg  |  9 +++++++++
 subdir/main.star             |  2 +-
 3 files changed, 26 insertions(+), 1 deletion(-)
 create mode 100644 subdir/generated/project.cfg
 create mode 100644 subdir/generated/realms.cfg

diff --git a/subdir/generated/project.cfg b/subdir/generated/project.cfg
new file mode 100644
index 0000000..3573c9a
--- /dev/null
+++ b/subdir/generated/project.cfg
@@ -0,0 +1,16 @@
+# Auto-generated by lucicfg.
+# Do not modify manually.
+#
+# For the schema of this file, see ProjectCfg message:
+#   https://config.luci.app/schemas/projects:project.cfg
+
+name: "lucicfg-example"
+lucicfg {
+  version: "1.45.0"
+  package_name: "@lucicfg-example"
+  package_dir: ".."
+  config_dir: "generated"
+  entry_point: "main.star"
+  experiments: "crbug.com/1347252"
+  experiments: "crbug.com/1496969"
+}
diff --git a/subdir/generated/realms.cfg b/subdir/generated/realms.cfg
new file mode 100644
index 0000000..d4c9015
--- /dev/null
+++ b/subdir/generated/realms.cfg
@@ -0,0 +1,9 @@
+# Auto-generated by lucicfg.
+# Do not modify manually.
+#
+# For the schema of this file, see RealmsCfg message:
+#   https://config.luci.app/schemas/projects:realms.cfg
+
+realms {
+  name: "@root"
+}
diff --git a/subdir/main.star b/subdir/main.star
index 99e4f86..88610df 100755
--- a/subdir/main.star
+++ b/subdir/main.star
@@ -1,3 +1,3 @@
 #!/usr/bin/env lucicfg
 
-
+luci.project(name="lucicfg-example")
